// Приклад масиву з розмірністю 200 та діапазоном значень від -50 до 50
let arr = [];
for (let i = 0; i < 200; i++) {
  arr.push(Math.floor(Math.random() * 101) - 50);  // Генерація випадкових чисел від -50 до 50
}

// Лічильник для пар з однаковими сусідніми значеннями
let count = 0;

// Перевірка кожної пари сусідніх елементів
for (let i = 0; i < arr.length - 1; i++) {
  if (arr[i] === arr[i + 1]) {
    count++;
  }
}

console.log("Кількість пар сусідніх елементів з однаковими значеннями: " + count);
